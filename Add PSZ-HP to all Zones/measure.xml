        <choice>
          <value>By Space Type's 'Standards Space Type'</value>
          <display_name>By Space Type's 'Standards Space Type'</display_name>
        </choice>
        <choice>
          <value>By Zone Filter</value>
          <display_name>By Zone Filter</display_name>
        </choice>
